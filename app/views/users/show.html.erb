<h2>User Info</h2>

<p>
  <b>First name:</b>
  <%=h @user.first_name %>
</p>

<p>
	<b>Middle name:</b>
	<%=h @user.middle_name %>
</p>

<p>
  <b>Last name:</b>
  <%=h @user.last_name %>
</p>

<p>
 <b>Username:</b>
	<%=h @user.username %>
</p>

<p>
  <b>Email:</b>
  <%=h @user.email %>
</p>

<!-- renders a table that shows all assignments for the user -->
<!-- do we want this to do more (aka manage assignments if can?)-->
	<% unless @user.assignments.nil? || @user.assignments.empty? || @user.is_admin? || @user.is_vp? %>
		<%= render :partial => "user_assignments" %>
	<% end %>





