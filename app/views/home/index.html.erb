<% if logged_in? %>
	<% if @user.is_admin? %>
		im the shit
	<% else %>
      <%= render :partial => "personal" %>
	<% end %>
<% else %>
      <div id= "login">
    <h1>Log In</h1>

    <% form_tag  sessions_path do -%>
    <p><%= label_tag 'login' %> &nbsp;   &nbsp; &nbsp;
    <%= text_field_tag 'login', @login %></p>

    <p><%= label_tag 'password' %>
    <%= password_field_tag 'password', nil %></p>

    <p><%= submit_tag 'Log in' %></p>
    <% end %>
      </div>
<%  end %>
