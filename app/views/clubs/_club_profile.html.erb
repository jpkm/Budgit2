<p id="notice"><%= notice %></p>

<h1><%= @club.name %>'s Profile:</h1>

<table class="datagrid">
	<thread class="datagrid">
    <th width="90">Year</th>
    <th width="60">Club</th>
	<th width="60">Active</th>
  </tr>

<% for account in @active_for_club %>
	<tr class="<%= cycle('dg_list_line_odd', 'dg_list_line_even') %>">
		<td align="left"><%= account.year %></td>
		<td align="left"><%= account.club.name %></td>
		<td align="left"><%= account.active %></td>
		<td><%= link_to 'Show', account %></td>
		<td><%= link_to 'Edit', edit_account_path(account) %></td>
		<td><%= link_to 'Destroy', account, :confirm => 'Are you sure?', :method => :delete %></td>
	</tr>
<% end %>
</table>
		


